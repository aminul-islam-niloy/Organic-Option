@model IEnumerable<IGrouping<string, OrganicOption.Models.Blogs.ProductPrice>>

@{
    ViewBag.Title = "Product Prices";
}
<br />
<h4>Product Prices</h4>
<br />
@foreach (var group in Model)
{
    <h3>@group.Key</h3>
    <table class="table">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Price</th>
                <th>Updated Date</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var item in group)
            {
                <tr>
                    <td>@item.ProductName</td>
                    <td>@item.Price Tk</td>
                    <td>@item.UpdatedDate.ToString("g")</td>
                    
                </tr>
            }
        </tbody>
    </table>
}
